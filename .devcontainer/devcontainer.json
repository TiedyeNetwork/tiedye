{
  "name": "Rust",
  "dockerFile": "Dockerfile",
  "runArgs": [
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash",
    "lldb.adapterType": "bundled",
    "lldb.executable": "/usr/bin/lldb"
  },
  "extensions": [
    "rust-lang.rust",
    "enfipy.plugin-polkadot",
    "bungcip.better-toml",
    "vadimcn.vscode-lldb"
  ]
}
